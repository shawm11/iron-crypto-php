{
    "name": "shawm11/iron-crypto",
    "description": "PHP implementation of the iron cryptographic utility",
    "type": "library",
    "license": "MIT",
    "version": "1.0.3",
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "php": ">=7.0.0",
        "codeception/assert-throws": "^1.0",
        "codeception/specify": "^1.1",
        "codeception/verify": "^1.0",
        "consolidation/robo": "^1.3",
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.2",
        "vierbergenlars/php-semver": "^3.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Shawm11\\Iron\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shawm11\\Iron\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}